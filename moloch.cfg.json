{
    "tools": {
        "subfinder": {
            "enabled": true,
            "flags": [
                "-all",
                "-recursive",
                "-d"
            ],
            "install_cmd": "go install -v github.com/projectdiscovery/subfinder/v2/cmd/subfinder@latest"
        },
        "amass": {
            "enabled": true,
            "flags": [
                "enum",
                "-passive",
                "-d"
            ],
            "install_cmd": "go install -v github.com/owasp-amass/amass/v4/...@master"
        },
        "assetfinder": {
            "enabled": true,
            "flags": [
                "--subs-only"
            ],
            "install_cmd": "go install github.com/tomnomnom/assetfinder@latest"
        },
        "findomain": {
            "enabled": true,
            "flags": [
                "-t"
            ],
            "install_cmd": "wget https://github.com/findomain/findomain/releases/latest/download/findomain-linux -O findomain && chmod +x findomain && sudo mv findomain /usr/local/bin/"
        },
        "chaos": {
            "enabled": true,
            "flags": [
                "-d",
                "-silent"
            ],
            "install_cmd": "go install -v github.com/projectdiscovery/chaos-client/cmd/chaos@latest"
        },
        "shuffledns": {
            "enabled": true,
            "flags": [
                "-silent",
                "-r",
                "8.8.8.8,1.1.1.1"
            ],
            "install_cmd": "go install -v github.com/projectdiscovery/shuffledns/cmd/shuffledns@latest"
        },
        "httpx": {
            "enabled": true,
            "flags": [
                "-silent",
                "-title",
                "-web-server",
                "-tech-detect",
                "-status-code",
                "-follow-redirects",
                "-random-agent",
                "-probe",
                "-timeout",
                "15",
                "-retries",
                "2",
                "-threads",
                "50",
                "-rate-limit",
                "300",
                "-content-length",
                "-method",
                "-websocket",
                "-pipeline",
                "-http2",
                "-response-time",
                "-jarm",
                "-asn",
                "-cdn",
                "-location"
            ],
            "install_cmd": "go install -v github.com/projectdiscovery/httpx/cmd/httpx@latest"
        },
        "nuclei": {
            "enabled": true,
            "flags": [
                "-silent",
                "-severity",
                "low,medium,high,critical",
                "-c",
                "100",
                "-rl",
                "300",
                "-timeout",
                "15",
                "-retries",
                "2",
                "-bulk-size",
                "25",
                "-max-host-error",
                "30",
                "-no-color",
                "-json",
                "-include-rr",
                "-duc",
                "-fhr"
            ],
            "community_templates": true,
            "custom_templates": true,
            "template_sources": [
                "~/nuclei-templates",
                "~/nuclei-community-templates",
                "~/custom-nuclei-templates"
            ],
            "exclude_tags": [
                "dos",
                "intrusive"
            ],
            "include_tags": [
                "cve",
                "osint",
                "tech",
                "default-logins",
                "exposures",
                "misconfiguration",
                "takeover"
            ],
            "install_cmd": "go install -v github.com/projectdiscovery/nuclei/v3/cmd/nuclei@latest"
        },
        "ffuf": {
            "enabled": true,
            "flags": [
                "-c",
                "-v",
                "-mc",
                "200,204,301,302,307,401,403,405,500",
                "-t",
                "50",
                "-recursion",
                "-of",
                "json"
            ],
            "install_cmd": "go install github.com/ffuf/ffuf/v2@latest"
        },
        "gobuster": {
            "enabled": true,
            "flags": [
                "dir",
                "-q",
                "-z",
                "-k"
            ],
            "install_cmd": "go install github.com/OJ/gobuster/v3@latest"
        },
        "katana": {
            "enabled": true,
            "flags": [
                "-silent",
                "-ps",
                "-f",
                "raw"
            ],
            "install_cmd": "go install github.com/projectdiscovery/katana/cmd/katana@latest"
        },
        "gau": {
            "enabled": true,
            "flags": [
                "--subs"
            ],
            "install_cmd": "go install github.com/lc/gau/v2/cmd/gau@latest"
        },
        "waybackurls": {
            "enabled": true,
            "flags": [],
            "install_cmd": "go install github.com/tomnomnom/waybackurls@latest"
        },
        "dalfox": {
            "enabled": true,
            "flags": [
                "-b",
                "your.xss.hunter.domain"
            ],
            "install_cmd": "go install github.com/hahwul/dalfox/v2@latest"
        },
        "arjun": {
            "enabled": true,
            "flags": [
                "-w",
                "default"
            ],
            "install_cmd": "pip3 install arjun"
        },
        "testssl": {
            "enabled": true,
            "flags": [
                "--quiet",
                "--warnings",
                "off",
                "--jsonfile-pretty"
            ],
            "install_cmd": "git clone --depth 1 https://github.com/drwetter/testssl.sh.git && cd testssl.sh && sudo ln -sf $(pwd)/testssl.sh /usr/local/bin/testssl.sh && cd .. && rm -rf testssl.sh"
        },
        "nikto": {
            "enabled": true,
            "flags": [
                "-C all",
                "-Format",
                "json"
            ],
            "install_cmd": "sudo apt install nikto -y || brew install nikto || echo 'Install nikto manually'"
        },
        "nmap": {
            "enabled": true,
            "flags": [
                "-sV",
                "-sC",
                "-Pn",
                "-T4",
                "--version-intensity",
                "5",
                "--script-timeout",
                "10m",
                "--host-timeout",
                "30m",
                "--min-rate",
                "100",
                "--max-rate",
                "1000",
                "--min-parallelism",
                "10",
                "--max-parallelism",
                "100",
                "--disable-arp-ping",
                "--source-port",
                "53",
                "--data-length",
                "32"
            ],
            "install_cmd": "sudo apt install nmap -y || brew install nmap || echo 'Install nmap manually'"
        },
        "dnsx": {
            "enabled": true,
            "flags": [
                "-silent",
                "-resp-only"
            ],
            "install_cmd": "go install -v github.com/projectdiscovery/dnsx/cmd/dnsx@latest"
        },
        "naabu": {
            "enabled": true,
            "flags": [
                "-silent",
                "-top-ports",
                "1000"
            ],
            "install_cmd": "go install -v github.com/projectdiscovery/naabu/v2/cmd/naabu@latest"
        },
        "tlsx": {
            "enabled": true,
            "flags": [
                "-silent",
                "-json"
            ],
            "install_cmd": "go install github.com/projectdiscovery/tlsx/cmd/tlsx@latest"
        },
        "cdncheck": {
            "enabled": true,
            "flags": [
                "-silent"
            ],
            "install_cmd": "go install github.com/projectdiscovery/cdncheck/cmd/cdncheck@latest"
        },
        "asnmap": {
            "enabled": true,
            "flags": [
                "-silent"
            ],
            "install_cmd": "go install github.com/projectdiscovery/asnmap/cmd/asnmap@latest"
        },
        "mapcidr": {
            "enabled": true,
            "flags": [
                "-silent"
            ],
            "install_cmd": "go install github.com/projectdiscovery/mapcidr/cmd/mapcidr@latest"
        },
        "gf": {
            "enabled": true,
            "flags": [],
            "install_cmd": "go install github.com/tomnomnom/gf@latest"
        },
        "unfurl": {
            "enabled": true,
            "flags": [],
            "install_cmd": "go install github.com/tomnomnom/unfurl@latest"
        },
        "anew": {
            "enabled": true,
            "flags": [],
            "install_cmd": "go install github.com/tomnomnom/anew@latest"
        },
        "notify": {
            "enabled": true,
            "flags": [
                "-silent"
            ],
            "install_cmd": "go install github.com/projectdiscovery/notify/cmd/notify@latest"
        },
        "interactsh-client": {
            "enabled": true,
            "flags": [
                "-o",
                "interactsh.log"
            ],
            "install_cmd": "go install github.com/projectdiscovery/interactsh/cmd/interactsh-client@latest"
        },
        "alterx": {
            "enabled": true,
            "flags": [
                "-silent"
            ],
            "install_cmd": "go install github.com/projectdiscovery/alterx/cmd/alterx@latest"
        }
    },
    "wordlists": {
        "subdomains": "subdomains-top1million-5000.txt",
